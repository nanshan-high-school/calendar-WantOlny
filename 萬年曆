#include <iostream>

int week;
int Ac();
int Bc();
int main()
{
	int year,tot,temp,temp2,temp3,temp4;
	printf("input year:\n");
	scanf("%d",year);
	if(year>1900)
	{
		temp=year-1900;
		temp2=temp/4;
		temp3=temp/100;
		temp4=temp/400;
		tot=temp+temp2-temp3+temp4;
		week=tot%7+1;
	}
	else
	{
		temp=1900-year;
		temp2=temp/4;
		temp3=temp/100;
		temp4=temp/400;
		tot=temp+temp2-temp3+temp4;
		week=tot%7+1;
	}
	if(year%400==0)
	{
		if(year%4==0)
		{
			if(year%100==0)
			{
				Ac();
			}
			else
			{
				Bc();
			}
		}
		else
		{
			Ac();
		}
	}
	else
	{
		Ac();
	}
	return 0;
}
Ac()
{
	int mon=1,day=1,i,j,k,l=0;
	while(mon<=12)
	{
		switch(mon)
		{
			case 1:
				for(i=0;i<week%7;i++)
				{
				printf("X");
				}
				for(j=0;j<7-i;j++)
				{
					printf("%d",day);
					day++;
				}
				k=day-1;
				while(day<=31)
				{
					if((day-k)%7==0)
					{
						printf("%d",day);
						printf("\n");
						day++;
						l++;
					}
